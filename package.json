{
  "name": "coding_cartel",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest",
    "eslint": "eslint ./pages/** ./models/** ./tests/** ./*.js --fix",
    "stylelint": "npx stylelint ./styles/** --fix",
    "lint": "npm-s eslint stylelint",
    "db:create:dev": "npx sequelize db:create --env=\"development\"",
    "db:create:test": "npx sequelize db:create --env=\"test\"",
    "db:create:prod": "npx sequelize db:create --env=\"production\"",
    "db:create:all": "npm-run-all db:create:dev db:create:test db:create:prod",
    "prepare": "husky install"
  },
  "dependencies": {
    "mysql2": "^2.2.5",
    "next": "10.2.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "sequelize": "^6.6.2"
  },
  "devDependencies": {
    "autoprefixer": "^10.2.5",
    "dotenv": "^10.0.0",
    "eslint": "^7.26.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-css-modules": "^2.11.0",
    "eslint-plugin-import": "^2.23.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^11.0.0",
    "postcss": "^8.2.15",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-nested": "^5.0.5",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.3.0",
    "sequelize-cli": "^6.2.0",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "tailwindcss": "^2.1.2"
  }
}
